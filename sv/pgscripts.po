msgid "  -l, --login               role can login (default)\n"
msgstr "  -l, --login               rollen kan logga in (standard)\n"

#: createuser.c:314
msgid "  -L, --no-login            role cannot login\n"
msgstr "  -L, --no-login            rollen kan inte logga in\n"

