msgid "  -E, --encrypted           encrypt stored password\n"
msgstr "  -E, --encrypted           spara lösenordet krypterat\n"

#: createuser.c:321
msgid "  -N, --unencrypted         do not encrypt stored password\n"
msgstr "  -N, --unencrypted         spara lösenordet okrypterat\n"

