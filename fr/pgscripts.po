msgid "Usage:\n"
msgstr "Usage:\n"

#: createdb.c:220
#, c-format
msgid "  %s [OPTION]... [DBNAME] [DESCRIPTION]\n"
msgstr "  %s [OPTION]... [NOMBASE] [DESCRIPTION]\n"

#: clusterdb.c:236 createdb.c:221 createlang.c:206 createuser.c:306
