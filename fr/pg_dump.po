#, c-format
msgid ""
"  -S, --superuser=NAME     specify the superuser user name to use for\n"
"                           disabling triggers\n"
msgstr ""
"  -S, --superuser=NOM      spécifie le nom du super-utilisateur à utiliser pour\n"
"                           désactiver les déclencheurs (triggers)\n"

