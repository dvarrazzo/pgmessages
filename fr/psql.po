"    [ GROUP BY expression [, ...] ]\n"
"    [ HAVING condition [, ...] ]\n"
"    [ { UNION | INTERSECT | EXCEPT } [ ALL ] select ]\n"
"    [ ORDER BY expression [ ASC | DESC | USING opérateur ] [, ...] ]\n"
"    [ LIMIT { nombre | ALL } ]\n"
