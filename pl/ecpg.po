msgstr "element nagłówka deskryptora \"%d\" nie istnieje"

#: descriptor.c:182
msgid "nullable is always 1"
msgstr "nullable jest zawsze 1"

#: descriptor.c:185
msgid "key_member is always 0"
msgstr "key_member jest zawsze 0"

