#, c-format
msgid "Usage:\n"
msgstr "사용법:\n"

#: createdb.c:246
#, c-format
msgid "  %s [OPTION]... [DBNAME] [DESCRIPTION]\n"
msgstr "  %s [옵션]... [DB이름] [설명]\n"

#: createdb.c:247
#: createlang.c:217
#: createuser.c:302
#: dropdb.c:142
#: droplang.c:376
#: dropuser.c:141
