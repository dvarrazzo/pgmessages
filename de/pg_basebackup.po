#, c-format
msgid ""
"\n"
"General options:\n"
msgstr ""
"\n"
"Allgemeine Optionen:\n"

#: pg_basebackup.c:131
#, c-format
msgid ""
"  -c, --checkpoint=fast|spread\n"
"                            set fast or spread checkpointing\n"
msgstr ""

#: pg_basebackup.c:133
#, c-format
msgid "  -l, --label=label         set backup label\n"
msgstr ""

