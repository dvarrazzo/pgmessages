#: plperl.c:238
msgid ""
"If true, trusted and untrusted Perl code will be compiled in strict mode."
msgstr ""
"Si es verdadero, se compilará código Perl confiable y no confiable en modo "
"«strict»."

#: plperl.c:246
msgid "Perl code to execute when the perl interpreter is initialized."
msgstr "Código Perl a ejecutar cuando el intérprete Perl es inicializado."

#: plperl.c:390 plperl.c:529 plperl.c:535 plperl.c:669 plperl.c:681
#: plperl.c:1252 plperl.c:1367 plperl.c:1423
#, c-format
msgid "%s"
msgstr "%s"

#: plperl.c:391
msgid "While executing PostgreSQL::InServer::SPI::bootstrap"
msgstr "Durante la ejecución de PostgreSQL::InServer::SPI::bootstrap"

#: plperl.c:528
msgid "while parsing perl initialization"
msgstr "mientras se interpretaba la inicialización de Perl"

#: plperl.c:534
msgid "while running perl initialization"
msgstr "mientras se ejecutaba la inicialización de Perl"

#: plperl.c:668
msgid "while executing PLC_SAFE_BAD"
msgstr "mientras se ejecutaba PLC_SAFE_BAD"

#: plperl.c:680
msgid "while executing PLC_SAFE_OK"
msgstr "mientras se ejecutaba PLC_SAFE_OK"

#: plperl.c:756 plperl.c:952
#, c-format
msgid "Perl hash contains nonexistent column \"%s\""
msgstr "el hash de Perl contiene el columna inexistente «%s»"

#: plperl.c:928
msgid "$_TD->{new} does not exist"
msgstr "$_TD->{new} no existe"

#: plperl.c:932
msgid "$_TD->{new} is not a hash reference"
msgstr "$_TD->{new} no es una referencia a un hash"

#: plperl.c:1167 plperl.c:1864
#, c-format
msgid "PL/Perl functions cannot return type %s"
msgstr "las funciones en PL/Perl no pueden retornar el tipo %s"

#: plperl.c:1179 plperl.c:1911
#, c-format
msgid "PL/Perl functions cannot accept type %s"
msgstr "funciones de PL/Perl no pueden aceptar el tipo %s"

#: plperl.c:1259
#, c-format
msgid "didn't get a GLOB from compiling %s via %s"
msgstr "no se obtuvo un GLOB en la compilación de %s a través de %s"

#: plperl.c:1475
msgid "set-valued function called in context that cannot accept a set"
msgstr ""
"se llamó a una función que retorna un conjunto en un contexto que no puede "
"aceptarlo"

#: plperl.c:1518
msgid ""
"set-returning PL/Perl function must return reference to array or use "
"return_next"
msgstr ""
"una función PL/Perl que retorna un conjunto debe retornar una referencia a "
"un array o usar return_next"

#: plperl.c:1551
msgid "composite-returning PL/Perl function must return reference to hash"
msgstr ""
"una función Perl que retorna un tipo compuesto debe retornar una referencia "
"a un hash"

#: plperl.c:1560
msgid ""
"function returning record called in context that cannot accept type record"
msgstr ""
"se llamó una función que retorna un registro en un contexto que no puede "
"aceptarlo"

#: plperl.c:1680
msgid "ignoring modified row in DELETE trigger"
msgstr "ignorando la tupla modificada en el disparador DELETE"

#: plperl.c:1688
msgid ""
"result of PL/Perl trigger function must be undef, \"SKIP\", or \"MODIFY\""
msgstr ""
"el resultado de la función disparadora en PL/Perl debe ser undef, «SKIP» o "
"«MODIFY»"

#: plperl.c:1798
msgid "out of memory"
msgstr "memoria agotada"

#: plperl.c:1855
msgid "trigger functions can only be called as triggers"
msgstr "las funciones disparadoras sólo pueden ser llamadas como disparadores"

#: plperl.c:2165
msgid "cannot use return_next in a non-SETOF function"
msgstr "no se puede utilizar return_next en una función sin SETOF"

#: plperl.c:2171
msgid ""
"SETOF-composite-returning PL/Perl function must call return_next with "
"reference to hash"
msgstr ""
"una función Perl que retorna SETOF de un tipo compuesto debe invocar "
"return_next con una referencia a un hash"

#: plperl.c:2958
#, c-format
msgid "PL/Perl function \"%s\""
msgstr "función PL/Perl «%s»"

#: plperl.c:2969
#, c-format
msgid "compilation of PL/Perl function \"%s\""
msgstr "compilación de la función PL/Perl «%s»"

#: plperl.c:2978
msgid "PL/Perl anonymous code block"
msgstr "bloque de código anónimo de PL/Perl"
